  <style>
    div.sec {
      margin: 16px;
      padding: 16px;
      width: 380px;
      box-shadow: 10px 5px 5px #ccc;
      border-left: 8px solid #3b1717;
      border-radius: 8px;
    }

    p.content {
      cursor: pointer;
      color: gold;
      font-family: sans-serif;
      transition: all 1000ms ease-in-out;
    }

    p:hover {
      color: brown;
      transform: scaleY(1.2);
      text-decoration: brown wavy underline;
    }

    span.inline::before {
      content: "before ";
      font-family: sans-serif;
    }

    span.inline::after {
      content: " after";
      font-family: sans-serif;
      color: chocolate;
    }

    div.shape {
      margin: 16px;
      padding: 16px;
      background: linear-gradient(127deg, rgba(96, 20, 93, 0.8), 70%, transparent);
      width: 360px;
      border-radius: 16px;
    }

    div.eclipse {
      margin: 16px;
      padding: 16px;
      background-image: radial-gradient(ellipse farthest-side at 80% 50%,#791111, #e9e9e9);
      width: 360px;
      border-radius: 16px;
    }

    div.eclipse:hover {
      transform: rotateY(160deg) skew(20deg);
    }

    div.container {
      position: relative;
      margin: 16px;
      height: 120px;
      border: 1px solid #3b1717;
    }

    div.banner {
      position: absolute;
      top: 1px;
      left: 1px;
      width: 128px;
      height: 32px;
      border: 1px solid #3c3a3a;
      transform: rotateZ(10deg);
    }
  </style>
